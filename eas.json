{
  "cli": {
    "version": ">= 16.18.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EAS_BUILD_DISABLE_COCOAPODS_CACHE": "1"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "GOOGLE_SERVICES_JSON": "@GOOGLE_SERVICES_JSON"
      }
    },
    "test": {
      "config": "test.yml"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "salaarnold14@gmail.com",
        "ascAppId": "6757685562"
      }
    }
  }
}
